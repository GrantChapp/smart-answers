<% if calculator.fully_protected_from_costs?%>
  <%= render partial: "fully_protected_from_costs", locals: {calculator: calculator} %>
<% elsif calculator.fully_repaid? %>
  <%= render partial: "fully_repaid", locals: {calculator: calculator} %>
<% else %>
  <%= render partial: "remaining_costs", locals: {calculator: calculator} %>
<% end %>
